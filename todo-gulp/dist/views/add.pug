extends layout
block content
	h1.
		#{title}
	form#formAdd(name="addList",method="post",action="/")
		p Number:
			input#inputNumber(type="number", placeholder="ex. 123456", name="number")
		p Text: 
			input#inputDato(type="text", placeholder="ex. what do you do?", name="dato")
		p
			button#btnSubmit(type="submit") submit